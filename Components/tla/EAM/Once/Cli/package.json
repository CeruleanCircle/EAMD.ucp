{
  "name": "@cerulean-circle/once-cli",
  "version": "0.0.1",
  "description": "once cli",
  "author": "phibar | CeruleanCircle",
  "bin": {
    "once": "./bin/run.mjs"
  },
  "type": "module",
  "homepage": "https://github.com/cerulean-circle/once-cli",
  "license": "AGPL-3.0",
  "main": "dist/index.mjs",
  "files": [
    "/bin",
    "/dist"
  ],
  "dependencies": {
    "@oclif/core": "^1.20.4",
    "@oclif/plugin-autocomplete": "^1.3.6",
    "@oclif/plugin-help": "^5.1.19",
    "@oclif/plugin-plugins": "^2.1.7"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-plugin-once":"workspace:*",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-plugin-oclif": "^0.1.0",
    "oclif": "^3.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3"
  },
  "oclif": {
    "bin": "once",
    "dirname": "once",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": ":"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint2": "eslint . --ext .ts --config .eslintrc",
    "postpack": "rimraf rm -f oclif.manifest.json",
    "posttest": "lint",
    "prepack": "pnpm build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/cerulean-circle/once-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
