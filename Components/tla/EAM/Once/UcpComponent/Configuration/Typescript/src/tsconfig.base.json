{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",                           
      "moduleResolution": "node",       
      "isolatedModules":true,      
      "sourceMap": true, 
      "declaration": true,                             
      "declarationMap": true,
      "removeComments": true,
      "esModuleInterop": true, 
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitAny": true,
      "skipLibCheck": true,
      "incremental": false,
    }
  }
  